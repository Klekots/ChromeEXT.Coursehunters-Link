{
  "name": "CH Fast Save",
  "version": "1.0",
  "description": "Fast video save in bookmarks",
  "permissions": ["storage", "bookmarks", "activeTab", "webNavigation"],
  "content_scripts":[
    {
      "matches": ["https://coursehunter.net/*"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action":{
  },
  "manifest_version": 2,
  "icons": { 
    "16": "./assets/images/ch16.png",
    "19": "./assets/images/ch19.png",
    "38": "./assets/images/ch38.png",
    "48": "./assets/images/ch48.png",
   "128": "./assets/images/ch128.png" 
  }
}